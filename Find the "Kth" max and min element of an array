import java.util.*;
class M
{
    public static void main(String ay[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the Number of Elements --> ");
        int n=sc.nextInt();
        int arr[]=new int[n];
        System.out.print("Now Enter the Elements --> ");
        for(int i=0;i<n;i++)
            arr[i]=sc.nextInt();
        Arrays.sort(arr);
        System.out.print("Enter the Value K --> ");
        int k=sc.nextInt();
        System.out.println("Kth Min Element is "+arr[k-1]);
        System.out.println("Kth Max Element is "+arr[n-k]);
    }
}
