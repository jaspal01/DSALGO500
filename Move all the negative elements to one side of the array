import java.util.*;
class M
{
    public static void main(String ay[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter the Number of the Elements to Enter --> ");
        int n=sc.nextInt();
        int arr[]=new int[n];
        System.out.print("Enter the Elements Now --> ");
        for(int i=0;i<n;i++)
            arr[i]=sc.nextInt();
        Arrays.sort(arr);
        System.out.print("Negative Element on one Side is : ");
        for(int i:arr)
            System.out.print(i+" ");
    }
}
